<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Admin</title>
    <!-- Bootstrap Core CSS -->
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Custom CSS -->

    <link href="~/Content/sb-admin.css" rel="stylesheet" />
    <!-- Custom Fonts -->
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <style type="text/css">
        html, body {
            max-width: 100%;
            overflow-x: hidden;
        }
    </style>
</head>
<body style="background-color:white;">
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Admin Panel</a>
            </div>
            <!-- Top Menu Items -->
            <ul class="nav navbar-right top-nav">
                @Html.Partial("_LoginPartial")

            </ul>
            <!-- /.navbar-collapse -->
        </nav>

        <!-- /#page-wrapper -->
    </div>
    <div class="row">
        <div class="col-md-2">
            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li>
                        <a href="@Url.Action("StudentList", "Student")"><i class="fa fa-fw fa-bar-chart-o"></i>Home</a>
                    <li>
                        <a href="javascript:;" data-toggle="collapse" data-target="#Assessments"><i class="fa fa-fw fa-arrows-v"></i> Assessment Reports <i class="fa fa-fw fa-caret-down"></i></a>
                        <ul id="Assessments" class="collapse">
                            <li class="active">
                                <a href="@Url.Action("GetALLAssessmentCategories", "StudentReport")"><i class="fa fa-fw fa-table"></i>Assessment Categories</a>
                            </li>
                            <li>
                                <a href="@Url.Action("GetALLAssessment", "StudentReport")"><i class="fa fa-fw fa-table"></i>Assessment Types</a>

                            </li>
                            <li>
                                <a href="@Url.Action("GetALLSubAssessment", "StudentReport")"><i class="fa fa-fw fa-table"></i>Assessment Sub Types</a>

                            </li>
                            <li>
                                <a href="@Url.Action("GetALLStudentGeneralAssessment", "StudentReport")"><i class="fa fa-fw fa-table"></i>Student General Assessment</a>

                            </li>
                            <li>
                                <a href="@Url.Action("GetALLStudentAcadmicAssessment", "StudentReport")"><i class="fa fa-fw fa-table"></i>Student Acadmic Assessment</a>

                            </li>
                            <li>
                                <a href="@Url.Action("GetStudentReport", "StudentReport")"><i class="fa fa-fw fa-table"></i>View Student Report</a>

                            </li>
                            
                            <li>
                                <a href="@Url.Action("GetALLTeacherGeneralAssessment", "StudentReport")"><i class="fa fa-fw fa-table"></i>Teacher General Assessment</a>

                            </li>
                            <li>
                                <a href="@Url.Action("GetTeacherReport", "StudentReport")"><i class="fa fa-fw fa-table"></i>View Teacher Report</a>

                            </li>
                            <li>
                                <a href="@Url.Action("GetTeacherReportByClass", "StudentReport")"><i class="fa fa-fw fa-table"></i>Teacher Report By Class</a>

                            </li>
                        </ul>


                    </li>
                    <li>
                        <a href="javascript:;" data-toggle="collapse" data-target="#Results"><i class="fa fa-fw fa-arrows-v"></i> Results <i class="fa fa-fw fa-caret-down"></i></a>
                        <ul id="Results" class="collapse">
                            <li>
                                <a href="@Url.Action("GetALLSocailDescriptions", "StudentResult")"><i class="fa fa-fw fa-table"></i>Social And Personal Description</a>
                            </li>
                            <li>
                                <a href="@Url.Action("GetALLStudyDescriptions", "StudentResult")"><i class="fa fa-fw fa-table"></i>Work And Study Description</a>
                            </li>
                            <li>
                                <a href="@Url.Action("AddChangesStudentResult", "StudentResult")"><i class="fa fa-fw fa-table"></i>Add Result</a>
                            </li>
                            <li>
                                <a href="@Url.Action("GetGeneralResultSheet", "StudentResult")"><i class="fa fa-fw fa-table"></i>View Result</a>
                            </li>
                            

                        </ul>


                    </li>

                </ul>
            </div>
        </div>
        <div class="col-md-10">
            <div class="container">
                <div class="body-content">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <!-- /#wrapper -->
    <!-- jQuery -->
    <script src="~/Scripts/jquery-2.2.1.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/Common.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>